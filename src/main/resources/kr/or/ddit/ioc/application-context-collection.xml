<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- kr.or.ddit.ioc.CollectionBean  스프링 빈 등록하기, 네가지 속성 등록하기  -->
	<bean id="collectionBean" class="kr.or.ddit.ioc.CollectionBean" >
		<property name="list">
			<list> 
				<value>brown</value>
				<value>cony</value>
				<value>sally</value>
			</list>
		</property>
		<property name="set">
			<set>
				<value>brown_set</value>
				<value>cony_set</value>
				<value>sally_set</value>
			</set>
		</property>
		<property name="map">
			<map>
				<entry key="name" value="brown"></entry>
				<entry key="age" value="15"></entry>
			</map>
		</property>
		<property name="property">
			<props>
				<prop key="db.userId">pc03</prop>
				<prop key="db.password">java</prop>
			</props>
		</property>
	</bean>
	
	

</beans>
